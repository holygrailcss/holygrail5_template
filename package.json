{
  "name": "holygrail5-template",
  "version": "1.0.0",
  "description": "Template moderno basado en Holy Grail Layout con generador de CSS y sistema de componentes",
  "homepage": "https://github.com/holygrailcss/holygrail5_template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holygrailcss/holygrail5_template.git"
  },
  "bugs": {
    "url": "https://github.com/holygrailcss/holygrail5_template/issues"
  },
  "scripts": {
    "dev": "bash scripts/dev.sh",
    "start": "serve dist -l 3000",
    "build": "npm run clean && bash scripts/build.sh",
    "clean": "rm -rf dist && mkdir -p dist/css dist/js",
    "tema": "bash scripts/theme.sh"
  },
  "devDependencies": {
    "serve": "^14.2.1",
    "holygrail5": "^1.0.8"
  },
  "keywords": [
    "holy-grail",
    "layout",
    "css",
    "grid",
    "responsive",
    "template",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
}

